        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>on_lift = false;

switch (state) {
    case base_state:
        lift_selected = level;
    
        if (ammo_delay &gt; 0)
            ammo_delay--;
        else {
            ammo += 0.5;
            if (ammo &gt; max_ammo)
                ammo = max_ammo;
        }
        
        if (instance_exists(obj_space) &amp;&amp; ! scr_item_found(item_air_tank))
            air--;
        else
            air = 200;
            
        if (air &lt;= 0) {
            audio_play_sound(snd_hurt, 0, false);
            hp = 0;
        }
            
        if (go_to_end &gt; 0) {
            go_to_end++;
            if (go_to_end &gt; 40) {
                persistent = false;
                obj_player.persistent = false;
                room_goto(rm_end);
            }
        }
        else if (hp &lt;= 0) {
            audio_stop_sound(mus_evil);
            audio_stop_sound(ds_list_find_value(music_list, level));
            persistent = false;
            if (instance_exists(obj_player))
                obj_player.persistent = false;
            instance_create(0, 0, obj_game_over);
        }
        else if (instance_exists(obj_player)) {
            mask_index = spr_player;
            if (place_meeting(obj_player.x, obj_player.y, obj_elevator))
                on_lift = true;
            
            if (keyboard_check_pressed(vk_shift) &amp;&amp; on_lift) {
                audio_play_sound(snd_lift, 0, false);
                state = lift_state;
                obj_player.canshoot = 10;
            }
            mask_index = spr_floor_base;
            
            var change_room = false;
        
            if (obj_player.x &lt; 0) {
                obj_player.x += room_width;
                room_x--;
                change_room = true;
            }
            else if (obj_player.x &gt; room_width) {
                obj_player.x -= room_width;
                room_x++;
                change_room = true;
            }
            else if (obj_player.y &lt; 0) {
                obj_player.y += room_height;
                room_y--;
                change_room = true;
            }
            else if (obj_player.y &gt; room_height) {
                obj_player.y -= room_height;
                room_y++;
                change_room = true;
            }
            
            if (change_room)
                room_goto(ds_grid_get(ds_list_find_value(level_list, level), room_x, room_y));
        }
        
        if (keyboard_check_pressed(vk_escape)) {
            audio_stop_sound(ds_list_find_value(music_list, level));
            audio_stop_sound(mus_evil);
            audio_play_sound(snd_cancel, 0, false);
            if (instance_exists(obj_player))
                obj_player.persistent = false;
            persistent = false;
            room_goto(rm_title);
        }
        
        if (keyboard_check_pressed(vk_space)) {
            audio_play_sound(snd_map, 0, false);
            state = map_state;
        }
        
        break;
    case item_state:
        if (keyboard_check_pressed(vk_anykey) &amp;&amp; !audio_is_playing(mus_fanfare)) {
            audio_resume_sound(ds_list_find_value(obj_game_control.music_list, obj_game_control.level));
            state = base_state;
        }
        
        break;
    case lift_state:
        if (keyboard_check_pressed(vk_up)) {
            audio_play_sound(snd_select, 0, false);
            lift_selected--;
        }
        if (keyboard_check_pressed(vk_down)) {
            audio_play_sound(snd_select, 0, false);
            lift_selected++;
        }
            
        if (lift_selected &lt; 0)
            lift_selected = 4;
        if (lift_selected &gt; 4)
            lift_selected = 0;
            
        if (keyboard_check_pressed(ord('A'))) {
            if (ds_list_find_index(unlocked_levels, lift_selected) == -1)
                audio_play_sound(snd_error, 0, false);
            else {
                audio_play_sound(snd_confirm, 0, false);
                state = base_state;
                if (lift_selected != level) {
                    audio_stop_sound(ds_list_find_value(music_list, level));
                    level = lift_selected;
                    obj_player.x = room_width / 2;
                    obj_player.y = room_height / 2;
                    room_goto(ds_grid_get(ds_list_find_value(level_list, level), room_x, room_y));
                }
            }
        }
        
        if (keyboard_check_pressed(vk_shift) || keyboard_check_pressed(vk_escape)) {
            audio_play_sound(snd_cancel, 0, false);
            state = base_state;
        }
            
        break;
    case map_state:
        if (keyboard_check_pressed(vk_up)) {
            audio_play_sound(snd_select, 0, false);
            lift_selected--;
        }
        if (keyboard_check_pressed(vk_down)) {
            audio_play_sound(snd_select, 0, false);
            lift_selected++;
        }
            
        if (lift_selected &lt; 0)
            lift_selected = 4;
        if (lift_selected &gt; 4)
            lift_selected = 0;
    
        if (keyboard_check_pressed(vk_space) || keyboard_check_pressed(vk_escape)) {
            audio_play_sound(snd_cancel, 0, false);
            state = base_state;
        }
        break;
}
