        <arguments>
          <argument>
            <kind>1</kind>
            <string>//constants
base_state = 0;
dash_state = 1;
magnet_state = 2;
phase_max = 5;

//variables
dir_x = 0;
dir_y = 0;
facing_x = 1;
facing_y = 1;
priority_x = true;
angle = 0;
right_gun = true;
canshoot = 0;
inv = 0;
flash = true;
state = base_state;
dash = 0;
dash_cooldown = 0;
phase = 0;
last_floor = -1;
magnet_flash = true;
magnet_dir = 1;
convey_x = 0;
convey_y = 0;
</string>
